{
	"name": "PS-backend",
	"version": "1.0.0",
	"description": "Express TypeScript backend with Prisma",
	"main": "dist/index.js",
	"scripts": {
		"dev": "ts-node-dev --respawn --transpile-only --exit-child --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" -r tsconfig-paths/register src/server.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"postbuild": "npm run prisma:generate",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate deploy",
		"prisma:studio": "prisma studio",
		"prisma:push": "prisma db push",
		"railway:setup": "npm run prisma:migrate",
		"test": "echo \"No tests yet\" && exit 0"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "^5.15.0",
    "@prisma/client": "^5.15.0",
    "app-root-path": "^3.1.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-zod-validator": "^1.0.1",
    "formidable": "^3.5.4",
    "fs-extra": "^11.3.3",
    "multer": "^2.0.2",
    "pg": "^8.18.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/formidable": "^3.4.6",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "prisma": "^5.15.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
